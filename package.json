{
	"name": "draw-libre-react",
	"version": "0.1.0",
	"description": "React wrapper for the draw-libre library.",
	"private": false,
	"type": "module",
	"scripts": {
	  "build": "tsup",
	  "build:w": "tsup --watch",
	  "dev": "tsc -w",
	  "lint": "tsc",
	  "prettier": "prettier --write .",
	  "exports": "attw --pack .",
	  "ci": "npm run lint && npm run prettier && npm run build",
	  "release": "npm run ci",
	  "prepublishOnly": "npm run ci"
	},
	"repository": {
	  "type": "git",
	  "url": "git+https://github.com/HusikyanLevon/draw-libre-react.git"
	},
	"keywords": [
	  "maplibre",
	  "mapbox",
	  "draw",
	  "draw-libre"
	],
	"author": "Usikian Levon <levonhusikyan@gmail.com>",
	"license": "MIT",
	"bugs": {
	  "url": "https://github.com/HusikyanLevon/draw-libre-react/issues"
	},
	"homepage": "https://github.com/HusikyanLevon/draw-libre-react#readme",
	"files": [
	  "dist"
	],
	"peerDependencies": {
		"draw-libre": "^0.5.0",
		"mapbox-gl": "^3.12.0",
		"maplibre-gl": "^5.6.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@types/react": "^19.1.8",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"main": "dist/index.js",
	"browser": "dist/index.js",
	"style": "dist/index.css",
	"exports": {
	  ".": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.js",
		"default": "./dist/index.js"
	  },
	  "./dist/index.css": "./dist/index.css"
	},
	"sideEffects": false
  }
  